{
    "compilerOptions": {
      "strict": false,
      "module": "NodeNext",
      "moduleResolution": "NodeNext",
      "target": "ES2020",
      "baseUrl": "src",
      "noImplicitAny": true,
      "sourceMap": true,
      "esModuleInterop": true,
      "outDir": "dist",
      "paths": {
        "@models/*": ["models/*"],
        "@schema/*": ["schema/*"],
        "@utils/*": ["utils/*"],
  }
    },
    "include": ["src/**/*"],
    "exclude": ["node_modules"]
  }